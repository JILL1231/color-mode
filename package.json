{
	"name": "color-conv",
	"displayName": "Color Conv",
	"description": "It is more convenient to change colors",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"color",
		"converter",
		"color conv",
		"color converter"
	],
	"activationEvents": [
		"onCommand:color-conv.color"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "color-conv.color",
				"category": "Color",
				"title": "color conv",
				"icon": "./icon/colorIcon.png"
			}
		],
		"keybindings": [
			{
				"command": "color-conv.color",
				"key": "ctrl+shift+e",
				"mac": "cmd+shift+e",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "editorTextFocus",
					"command": "color-conv.color",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "color-conv.color",
					"group": "navigation",
					"when": "editorTextFocus"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}